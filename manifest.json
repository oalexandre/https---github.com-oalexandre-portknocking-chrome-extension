{
    "name": "Port Knocking",
    "description": "A simple port knocking Chrome Extension usualily used to open a port in a firewall. This is a free and open source software powered Aupi",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
        "16": "app_icon.png",
        "32": "app_icon.png",
        "48": "app_icon.png",
        "128": "app_icon.png"
      },

    "action": {
        "default_popup": "popup.html"

    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}